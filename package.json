{
  "name": "easeinventory.com",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --project=e2e-chromium",
    "test:e2e:all": "playwright test",
    "test:e2e:report": "playwright show-report test-results/report",
    "services": "tsx packages/functions/src/dev-server.ts",
    "infra:up": "docker compose up -d",
    "infra:down": "docker compose down",
    "infra:logs": "docker compose logs -f",
    "dev:full": "docker compose up -d && npm run dev",
    "sst:dev": "sst dev",
    "sst:deploy": "sst deploy",
    "sst:remove": "sst remove",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "db:push": "prisma db push",
    "db:push:local": "DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:studio:local": "DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres prisma studio",
    "dev:local": "npm run supabase:start && npm run db:push:local && npm run dev"
  },
